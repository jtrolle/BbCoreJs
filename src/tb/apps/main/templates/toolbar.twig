
<nav id="bb5-navbar-primary" class="navbar clearfix" role="navigation">
    <span class="navbar-brand"><img src="{{ logo }}" alt="BackBee"></span>
    <ul class="nav bb5-ui-width-setter" id="bb5-maintabs">
        {% for menu in menus %}
            <li{% if menu.items %} class="dropdown"{% endif %}>
                <a class="btn{% if menu.items %} dropdown-toggle{% endif %}" id="{{ menu.label }}-menu" href="{{ menu.url }}">
                    {{ menu.text }}
                </a>
                {% if menu.items %}
                    <ul aria-labelledby="{{ menu.label }}" role="menu" class="dropdown-menu">
                        {% for item in menu.items %}
                            <li>
                                <a data-toggle="tab" class="bb-item-menu" tabindex="-1" href="{{ item.url }}">{{ item.text }}</a>
                            </li>
                        {% endfor %}
                    </ul>
                {% endif %}
            </li>
        {% endfor %}
    </ul>
    <ul class="nav navbar-nav pull-right"></ul>
</nav>

<div class="bb5-ui-width-setter bb5-ui-banner">
    <div class="tab-content" id="bb5-maintabsContent"></div>
</div>

<div class="bb5-content-breadcrumb">
    <div class="bb5-ui-width-setter">

    </div>
</div>
<div class="bb5-dialog-container"></div>
<section id="bb-box-wrapper"></section>
